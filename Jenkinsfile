pipeline {
    agent any

    stages {
        stage('one') {
            steps {
                echo 'Hello World'
                sleep 3
            }
        }
        stage('two') {
            steps {
                echo 'Hello World1'
                sleep 9
            }
        }
        stage('three') {
            steps {
                echo 'Hello World2'
                sleep 5
            }
        }
    }
}
